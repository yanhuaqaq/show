function slideshow(t,n){function o(){var n=e[i];i=++i%e.length;var o=e[i],c=s[i];n.fadeTo(800,0,function(){$(this).attr("src",c)}),o.fadeTo(800,1),a=a%t+1}console.log("init slideshow");var e=[],s=[],i=0,a=1;$("#homeslide img").each(function(){e.push($(this)),s.push($(this).attr("src"))}),window.setInterval(o,n)}(function(){document.addEventListener("DOMContentLoaded",function(){return console.log("start slideshow"),slideshow(3,6e3)})}).call(this);